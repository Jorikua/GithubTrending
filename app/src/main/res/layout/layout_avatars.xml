<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/tvBuiltBy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/built_by"
        android:textSize="@dimen/text_size_small"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/iv1"
        android:layout_width="@dimen/iv_small_size"
        android:layout_height="@dimen/iv_small_size"
        android:layout_marginStart="@dimen/margin_vsmall"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@id/tvBuiltBy"
        app:layout_constraintTop_toTopOf="parent"
        tools:src="@drawable/ic_favourites" />

    <ImageView
        android:id="@+id/iv2"
        android:layout_width="@dimen/iv_small_size"
        android:layout_height="@dimen/iv_small_size"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@id/iv1"
        app:layout_constraintStart_toEndOf="@id/iv1"
        app:layout_constraintTop_toTopOf="parent"
        tools:src="@drawable/ic_star" />

    <ImageView
        android:id="@+id/iv3"
        android:layout_width="@dimen/iv_small_size"
        android:layout_height="@dimen/iv_small_size"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@id/iv2"
        app:layout_constraintStart_toEndOf="@id/iv2"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/iv4"
        android:layout_width="@dimen/iv_small_size"
        android:layout_height="@dimen/iv_small_size"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@id/iv3"
        app:layout_constraintStart_toEndOf="@id/iv3"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/iv5"
        android:layout_width="@dimen/iv_small_size"
        android:layout_height="@dimen/iv_small_size"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@id/iv4"
        app:layout_constraintStart_toEndOf="@id/iv4"
        app:layout_constraintTop_toTopOf="parent" />

</merge>